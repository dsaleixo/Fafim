	public static void define_map(String i) {
		if(i.equals( "0")) {
			max_cicle=1000;
			path_map = "maps/mapadavid2.xml";
		}
		if(i.equals( "1")) {
			max_cicle=2500;
		
			path_map = "maps/8x8/basesWorkers8x8A.xml";
		}
		if(i.equals("2")) {
			max_cicle=1500;
			path_map = "maps/NoWhereToRun9x8.xml";
		}
		if(i.equals( "3")) {
			max_cicle=3000;
			path_map = "maps/24x24/basesWorkers24x24A.xml";
		}
	}
	
	
